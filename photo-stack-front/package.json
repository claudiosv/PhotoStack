{
  "name": "photo-stack-front",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.1.23",
    "apollo-cache-inmemory": "^1.4.0",
    "apollo-client": "^2.4.9",
    "apollo-link-http": "^1.5.9",
    "bloomer": "^0.6.5",
    "bulma": "^0.7.2",
    "eslint": "5.12.0",
    "fetch-progress": "^1.0.2",
    "formik": "^1.4.2",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.6.3",
    "react-file-drop": "^0.2.7",
    "react-grid-gallery": "^0.5.3",
    "react-image-gallery": "^0.8.12",
    "react-photo-gallery": "^6.3.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-select": "^2.2.0",
    "xhr-file": "^0.8.4",
    "yup": "^0.26.6"
  },
  "scripts": {
    "start": "./node_modules/.bin/react-scripts start",
    "build": "react-scripts build",
    "test": "xo && react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "xo-react"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "xo": "^0.23.0"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": [
      "xo-react"
    ],
    "env": [
      "browser",
      "es6"
    ],
    "rules": {
      "import/no-unassigned-import": {
        "allow": [
          "**/*.css"
        ]
      },
      "unicorn/filename-case": [
        "error",
        {
          "case": "pascalCase"
        }
      ]
    }
  }
}
