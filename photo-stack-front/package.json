{
  "name": "photo-stack-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bloomer": "^0.6.5",
    "bulma": "^0.7.2",
    "lodash": "^4.17.11",
    "node-sass": "^4.10.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropzone": "^8.0.3",
    "react-flip-toolkit": "^6.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "throttle-debounce": "^2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "xo && react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "xo-react"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "xo": "^0.23.0"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": [
      "xo-react"
    ],
    "env": [
      "browser",
      "es6"
    ],
    "rules": {
      "import/no-unassigned-import": {
        "allow": [
          "**/*.css"
        ]
      },
      "unicorn/filename-case": [
        "error",
        {
          "case": "pascalCase"
        }
      ]
    }
  }
}
